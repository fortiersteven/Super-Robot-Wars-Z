      <EnglishText/>
      <Notes/>
      <SpeakerId>14</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5104</PointerOffset>
      <JapaneseText>「どういう事なんだ？
