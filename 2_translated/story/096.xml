      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5712</PointerOffset>
      <JapaneseText>「どうなってるんだ、いったい…」</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>13</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
