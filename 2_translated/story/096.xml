      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5648</PointerOffset>
