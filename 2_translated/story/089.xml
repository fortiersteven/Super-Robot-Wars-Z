      <EnglishText/>
      <Notes/>
      <SpeakerId>13</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5920</PointerOffset>
