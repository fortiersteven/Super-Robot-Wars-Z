      <EnglishText/>
      <Notes/>
      <SpeakerId>34</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4720</PointerOffset>
      <JapaneseText>「ちょっと…かい？
