      <Notes/>
      <SpeakerId>39</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5040</PointerOffset>
      <JapaneseText>「アキって
