      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4368</PointerOffset>
      <JapaneseText>「その連中が
