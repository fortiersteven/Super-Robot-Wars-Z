      <Notes/>
      <SpeakerId>35</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4752</PointerOffset>
      <JapaneseText>「そのロジャーさんって？」</JapaneseText>
