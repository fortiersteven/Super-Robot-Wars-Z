      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5008</PointerOffset>
      <JapaneseText>「でもよ、あの兄ちゃんが
