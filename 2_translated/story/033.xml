      <EnglishText/>
      <Notes/>
      <SpeakerId>36</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5296</PointerOffset>
