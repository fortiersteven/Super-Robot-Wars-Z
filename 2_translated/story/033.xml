      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4592</PointerOffset>
      <JapaneseText>「どうやら、そいつの妹さんが
