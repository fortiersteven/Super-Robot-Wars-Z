      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3536</PointerOffset>
      <JapaneseText>「まあ、そんなポンコツに頼まなくても
