      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Chapter>Uncategorized</Chapter>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4848</PointerOffset>
      <JapaneseText>「黒いロボット？」</JapaneseText>
